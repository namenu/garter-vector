{
  "name": "re-vector",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true,
    "suffix": ".mjs"
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "bisect_ppx"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest"
  ],
  "ppx-flags": [
    "bisect_ppx/ppx"
  ],
  "warnings": {
    "error": "+101"
  },
  "refmt": 3,
  "gentypeconfig": {
    "language": "typescript",
    "debug": {
      "all": false,
      "basic": false
    }
  }
}